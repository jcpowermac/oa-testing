[rhel-7-server-ose-{{ target_version }}-{{ build_version }}-devel-rpms]
name = A repository of dependencies for Atomic OpenShift {{ target_version }}
baseurl = https://mirror.openshift.com/enterprise/all/{{ target_version }}/{{ build_version }}/x86_64/os/
failovermethod = priority
gpgcheck = 0
sslclientcert = /var/lib/yum/ops-mirror.pem
sslclientkey = /var/lib/yum/ops-mirror.pem
sslverify = 0
enabled = 1

