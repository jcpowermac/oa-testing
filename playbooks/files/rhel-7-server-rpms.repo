[rhel-7-server-releases]
name = Red Hat Enterprise Linux $majorrelease - Releases
baseurl = https://mirror.openshift.com/libra/rhui-rhel-server-$majorrelease-releases/
failovermethod = priority
gpgcheck = 0
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-redhat-release file:///etc/pki/rpm-gpg/RPM-GPG-KEY-redhat-beta https://mirror.openshift.com/libra/keys/RPM-GPG-KEY-redhat-openshifthosted
sslclientcert = /var/lib/yum/client-cert.pem
sslclientkey = /var/lib/yum/client-key.pem
sslverify = 0
enabled = 1

